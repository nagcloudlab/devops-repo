pipeline {
    agent any
     environment { 
        registry = "nagabhushanamn/backend-service" 
        registryCredential = 'dockerhub' 
        dockerImage = '' 
    }
    stages {
        stage('checkout from scm') {
            steps {
                echo 'checkout..'
            }
        }
        stage('build') {
            steps {
                dir("backend-service"){
                    sh 'mvn clean package'
                }
            }
        }
    }  
}